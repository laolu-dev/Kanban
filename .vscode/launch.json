{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug-Dev",
            "cwd": "frontend",
            "request": "launch",
            "type": "dart",
            "args": [
                "--flavor",
                "dev",
                "--target",
                "lib/main_dev.dart",
                "--dart-define-from-file",
                ".env.dev"
            ]
        },
        {
            "name": "Release-Dev",
            "cwd": "frontend",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--flavor",
                "dev",
                "--target",
                "lib/main_dev.dart",
                "--dart-define-from-file",
                ".env.dev"
            ]
        },
        {
            "name": "Release-Prod",
            "cwd": "frontend",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--flavor",
                "prod",
                "--target",
                "lib/main_prod.dart",
                "--dart-define-from-file",
                ".env.prod"
            ]
        },
    ]
}